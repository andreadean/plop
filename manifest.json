{
  "manifest_version": 2,

  "name": "Pool to poker and poker to pool",
    "short_name": "plop",
  "description": "Poker should be called pool and pool should be called poker",
  "author": "Andrea",
  "version": "0.0.2",

  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },

  "content_scripts":
  [
    {
      "matches": ["*://*/*"],
      "all_frames": true,
      "js": ["content_script.js"],
      "run_at": "document_end"
    }
  ]
}